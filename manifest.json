{
  "manifest_version": 3,
  "name": "Remove Japanese History in YouTube",
  "version": "1.0",
  "permissions": [
    "scripting",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://myactivity.google.com/*"
  ],
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://myactivity.google.com/product/youtube"
      ],
      "js": [
        "removeHistoryInActivity.js"
      ]
    },
    {
      "matches": [
        "https://www.youtube.com/feed/history"
      ],
      "js": [
        "removeHistoryInFeed.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "config.html",
        "config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}